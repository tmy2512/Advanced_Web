<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ManagementAssistanceForBusinessWeb_OnlyRole</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/styleVerticalNavbar.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/styleHorizontalNavbar.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/index.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/customLayout.css" asp-append-version="true" />
    <link rel="stylesheet" type="text/css" href="@ViewData["Title"]" />
    @if (ViewData["Title"] != null)
    {
                <link rel="stylesheet" type="text/css" href="@ViewData["Title"]" />
    }
    <link rel="stylesheet" href="~/ManagementAssistanceForBusinessWeb_OnlyRole.styles.css" asp-append-version="true" />
    <link
      href="https://fonts.googleapis.com/css2?family=Source+Sans+Pro:wght@400;600;700&display=swap"
      rel="stylesheet"/>
      >
</head>
<body>
    @Html.Partial("Partials/_HorizontalNavbar")

    @Html.Partial("Partials/_Verticalnavbar")
<div class="main">
        <main role="main" class="content" >
            @RenderBody()
        </main>
    </div>
  

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
     <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
     <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
